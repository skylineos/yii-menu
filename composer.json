{
    "name": "skylineos/yii-menu",
    "type": "yii2-extension",

    "description": "Yii2 extention for creating and managing menus",
    "keywords": [
        "yii2", 
        "bootstrap 4",
        "menu"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/skylineos/yii-menu/issues",
        "source": "https://github.com/skylineos/yii-menu"
    },
    "authors": [
        {
            "name": "Damien Kemens",
            "email": "dkemens@skylinenet.net"
        }
    ],
    "require": {
        "php": "~8.0.0",
        "yiisoft/yii2": "~2.0.0",
        "himiklab/yii2-sortable-grid-view-widget": "^1.0.11"
    },
    "autoload": {
        "psr-4": {
            "skylineos\\yii\\menu\\": "src"
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true    
}